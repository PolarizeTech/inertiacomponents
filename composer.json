{
  "name": "joshuaanderton/inertiakit",
  "description": "Tools for making great SaaS apps with Laravel and Inertia.js (using React and TypeScript)",
  "license": "MIT",
  "authors": [
      {
          "name": "Joshua Anderton",
          "email": "hey@joshuaanderton.ca"
      }
  ],
  "require": {
      "php": "^8.1|^8.2"
  },
  "autoload": {
      "psr-4": {
          "InertiaKit\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "InertiaKit\\Tests\\": "tests"
      }
  },
  "config": {
      "sort-packages": true
  },
  "extra": {
      "laravel": {
          "providers": [
              "Ja\\Inertia\\Providers\\ServiceProvider"
          ]
      }
  }
}
